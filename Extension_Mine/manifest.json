{
  "name": "extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "webRequest",
    "storage","tabs", "activeTab",
    "webRequestAuthProvider",
    "declarativeNetRequest"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["webScraperMutated.js","webScraperEntire.js","views/*.html", "scripts/*.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
